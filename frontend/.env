# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_API_TIMEOUT=10000

# Authentication
REACT_APP_AUTH_TOKEN_NAME=unus_auth_token
REACT_APP_JWT_EXPIRATION_TIME=900000 # 15 minutes in milliseconds

# Security
REACT_APP_ENABLE_CONTENT_SECURITY_POLICY=true
REACT_APP_ENABLE_STRICT_TRANSPORT_SECURITY=true

# Feature Flags
REACT_APP_ENABLE_FEATURE_X=true
REACT_APP_ENABLE_FEATURE_Y=false

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X

# External Services
REACT_APP_EXTERNAL_SERVICE_URL=https://api.example.com

# Deployment
REACT_APP_DEPLOYMENT_ENVIRONMENT=development

# Pagination
REACT_APP_ITEMS_PER_PAGE=20

# Date and Time
REACT_APP_DATE_FORMAT=YYYY-MM-DD
REACT_APP_TIME_ZONE=UTC

# Websocket
REACT_APP_WEBSOCKET_URL=ws://localhost:5000

# Public URL (if your app is not hosted at the root of the domain)
REACT_APP_PUBLIC_URL=/

# Error Reporting
REACT_APP_ERROR_REPORTING_SERVICE=sentry
REACT_APP_SENTRY_DSN=https://your-sentry-dsn.ingest.sentry.io/project-id

# Localization
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_SUPPORTED_LANGUAGES=en,es,fr

# Performance
REACT_APP_ENABLE_PERFORMANCE_TRACKING=true

# Development Tools
REACT_APP_ENABLE_REDUX_DEVTOOLS=false

# API Versioning
REACT_APP_API_VERSION=v1

# Rate Limiting
REACT_APP_API_RATE_LIMIT=100 # requests per minute

# Cache Control
REACT_APP_CACHE_MAX_AGE=3600 # in seconds

# Any other environment-specific variables
# REACT_APP_SOME_OTHER_VARIABLE=value
